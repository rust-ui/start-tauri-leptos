{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "test_tauri",
  "version": "0.1.0",
  "identifier": "com.test_tauri.dev",
  "build": {
    "frontendDist": "../target/site",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "cd .. && cargo leptos watch",
    "beforeBuildCommand": "cd .. && ./build-binaries.sh"
  },
  "app": {
    "windows": [
      {
        "title": "Test Tauri App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../target/site": "site/",
      "../target/site/pkg": "site/pkg/",
      "../Cargo.toml": "./"
    },
    "externalBin": [
      "../target/release/server"
    ]
  }
}